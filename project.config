[project]
	description = repo Janusza
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group Janusze
	owner = group user/eszejan
[access "refs/for/refs/*"]
	read = group Janusze
	read = group user/eszejan
	push = group Janusze
	push = group user/eszejan
	label-Code-Review = -2..+2 group Janusze
	label-Code-Review = -2..+2 group user/eszejan
	label-Verified = -1..+1 group Janusze
	label-Verified = -1..+1 group user/eszejan
	rebase = group Janusze
	rebase = group user/eszejan
	removeReviewer = group Janusze
	removeReviewer = group user/eszejan
	submit = group Janusze
	submit = group user/eszejan
[access "refs/heads/*"]
	read = group Janusze
	read = group user/eszejan
	create = group Janusze
	create = group user/eszejan
	push = group Janusze
	push = +force group user/eszejan
	pushMerge = group Janusze
	pushMerge = group user/eszejan
[access "refs/tags/*"]
	push = group Janusze
	push = +force group user/eszejan
	pushTag = group Janusze
	pushTag = group user/eszejan
	pushSignedTag = group Janusze
	pushSignedTag = group user/eszejan
